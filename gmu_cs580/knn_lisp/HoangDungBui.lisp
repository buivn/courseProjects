(defun difference (list1 list2 &key (measure 'count))
  ;;using the recursive method
  (if (or (null list1) (null list2)) ; if two input lists are empty, return 0
      0
      (cond ((equal measure 'squared);; if the measure is squared
	     (+ (* (- (first list1) (first list2)) (- (first list1) (first list2)))
		(difference (rest list1) (rest list2) :measure measure)))
	    ((equal measure 'count) ;; if the measure is count
	     (+ (if (= (first list1) (first list2))
		    0
		    1)
		(difference (rest list1) (rest list2) :measure measure)))
	    ((equal measure 'manhattan) ; if the measure is manhattan
	     (+ (abs (- (first list1) (first list2)))
		(difference (rest list1) (rest list2) :measure measure))))))

(defun k-nearest-neighbor (examples new-example &key (k 1) (measure 'count))
  ;; define the local variables
  (let ((diff-list nil) rl diff label knn (dem 0) (rep 0) (sum-error 0.0))
    (dolist (example examples) ;; run for all item in examples list
      ;; calculate the diffirence
      (setf diff (difference (first example) (first new-example) :measure measure))
      ;; save the label of that item to label
      (setf label (first (first (rest example))))
      (setf rl (reverse diff-list)) ;; reverse the current difference list
      ;; push the new item into the diff-list
      (if (null diff-list)
	 (setf diff-list (list (list diff label)))
	 (setf diff-list (reverse (push (list diff label) rl)))))
    ;; sort and filter the k item with smallest difference
    (setf knn (subseq (sort diff-list #'< :key #'first) 0 k))
    (dolist (case knn)
      (if (= (first (rest case)) 0.1)
	  (setf dem (+ dem 1)) ;; voting 
	  (setf rep (+ rep 1))) 
      (setf sum-error (+ sum-error diff))) ;; sum the error
    (if (> dem rep) ;; push out the voting result
	(list 0.1 sum-error)
	(list 0.9 sum-error))))

      
(defun generalization (training-examples test-examples &key (k 1) (measure 'count))
  (let ((correct 0) (incorrect 0)) ;; local variables
    (dolist (test-example test-examples)  ;; a loop to categorize a list of item in test examples 
      ;; checking the prediction result and the label  
      (if (equalp (first (first (rest test-example)))
		  (first (k-nearest-neighbor training-examples test-example :k k :measure measure)))
	  (setf correct (+ correct 1))
	  (setf incorrect (+ incorrect 1))))
    (* 100 (/ correct (+ correct incorrect))))) ;; return the correct percentage 


